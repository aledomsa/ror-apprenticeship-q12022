<p id="notice"><%= notice %></p>

<h1>Pokemons</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Ability list</th>
      <th>Height</th>
      <th>Image list</th>
      <th>Move list</th>
      <th>Name</th>
      <th>Pokemon</th>
      <th>Stat list</th>
      <th>Type list</th>
      <th>Url</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pokemons.each do |pokemon| %>
      <tr>
        <td><%= pokemon.id %></td>
        <td><%= pokemon.ability_list %></td>
        <td><%= pokemon.height %></td>
        <td><%= pokemon.image_list %></td>
        <td><%= pokemon.move_list %></td>
        <td><%= pokemon.name %></td>
        <td><%= pokemon.pokemon_id %></td>
        <td><%= pokemon.stat_list %></td>
        <td><%= pokemon.type_list %></td>
        <td><%= pokemon.url %></td>
        <td><%= pokemon.weight %></td>
        <td><%= link_to 'Show', pokemon %></td>
        <td><%= link_to 'Edit', edit_pokemon_path(pokemon) %></td>
        <td><%= link_to 'Destroy', pokemon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pokemon', new_pokemon_path %>
