<p id="notice"><%= notice %></p>

<h1>Pokemons</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Abilities</th>
      <th>Height</th>
      <th>Images</th>
      <th>Moves</th>
      <th>Name</th>
      <th>Pokemon</th>
      <th>Stats</th>
      <th>Types</th>
      <th>Url</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pokemons.each do |pokemon| %>
      <tr>
        <td><%= pokemon.id %></td>
        <td><%= pokemon.abilities %></td>
        <td><%= pokemon.height %></td>
        <td><%= pokemon.images %></td>
        <td><%= pokemon.moves %></td>
        <td><%= pokemon.name %></td>
        <td><%= pokemon.pokemon_id %></td>
        <td><%= pokemon.stats %></td>
        <td><%= pokemon.types %></td>
        <td><%= pokemon.url %></td>
        <td><%= pokemon.weight %></td>
        <td><%= link_to 'Show', pokemon %></td>
        <td><%= link_to 'Edit', edit_pokemon_path(pokemon) %></td>
        <td><%= link_to 'Destroy', pokemon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pokemon', new_pokemon_path %>
